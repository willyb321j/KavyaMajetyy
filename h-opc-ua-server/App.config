<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!--依赖注入注册配置节点-->
    <section name="injectionRegisterConfiguration" type="SD.IOC.Core.InjectionRegisterConfiguration, SD.IOC.Core" />
  </configSections>

  <!--应用程序配置-->
  <appSettings>
    <!--服务名称-->
    <add key="ServiceName" value="OPC.UA.Server-v1.0" />
    <!--服务显示名称-->
    <add key="ServiceDisplayName" value="OPC.UA.Server-v1.0" />
    <!--服务描述-->
    <add key="ServiceDescription" value="OPC-UA服务器v1.0" />
  </appSettings>

  <!--依赖注入注册配置-->
  <injectionRegisterConfiguration>
    <!--As接口程序集列表-->
    <asInterfaceAssemblies>
      <assembly name="h-opc-ua-server" />
    </asInterfaceAssemblies>
  </injectionRegisterConfiguration>
</configuration>
